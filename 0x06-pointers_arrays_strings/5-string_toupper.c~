#include "main.h"

/**
 * string_upper - changes a string to uppercase.
 * @ch: pointer of ch.
 * Return: character.
 */

char *string_toupper(char *ch)
{
	int i;

	for (i = 0; *(ch + i) != '\0'; i++)
	{



		if (*(ch + i) >= 97 && *(ch + i) <= 122)
		{
			*(ch + i) = *(ch + i) - ' ';

		}
		return (ch);


	}
}
